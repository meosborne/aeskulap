#
#	Makefile for dcmdata/include
#


@SET_MAKE@

SHELL = /bin/sh
srcdir = @srcdir@
top_srcdir = @top_srcdir@
configdir = @top_srcdir@/@configdir@

include $(configdir)/@common_makefile@


all:

install:
	$(configdir)/mkinstalldirs $(includedir)/dcmdata
	for file in *.h ; do \
		$(INSTALL_DATA) $$file $(includedir)/dcmdata ;\
	done

clean:
	rm -f $(TRASH)

distclean:
	rm -f $(DISTTRASH)
